  "author": "manufosela",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/manufosela/system-capabilities.git"
  },
  "dependencies": {
    "js-yaml": "^4.1.0"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^25.0.7",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "rollup": "^4.9.6"
